#include <stdio.h>

int main()
{
    int i,j;
  
    int n;
    n=3;
    for(i=1;i<=n;i++)
    {
        printf("%d",n);
    }
    printf("\n");
    for(i=1;i<n;i++)
    {
        printf("%d",n);
        printf("%d",i);
        printf("%d",n);
        printf("\n");
    }

    for(i=1;i<=n;i++)
    {
        printf("%d",n);
    }
    return 0;
}
